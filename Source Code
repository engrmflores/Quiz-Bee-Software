Label2.Text = "A. Managerial risks" + vbCrLf + "B. Technology risks" + vbCrLf + "C. Estimation risks" + vbCrLf + "D.Organizational risks"
ElseIf String.Equals(Label1.Text, "2. Which of the following risks are derived from the software or hardware technologies that are used to develop the system?") Then 'hi
