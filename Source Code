Private Sub easy_Load(sender As Object, e As EventArgs) Handles MyBase.Load
Label1.Text = "1.  Which three framework activities are present in Adaptive Software Development(ASD)?"
Label2.Text = "A. speculation, collaboration, learning" + vbCrLf + "B. analysis, design, coding" + vbCrLf + "C. requirements gathering, adaptive cycle planning, iterative development" + vbCrLf + "D. all of the mentioned"
Bt.Visible = "False"
End Sub
Private Sub Ab_Click(sender As Object, e As EventArgs) Handles Ab.Click
If String.Equals(Label1.Text, "1.  Which three framework activities are present in Adaptive Software Development(ASD)?") Then 'hello
p = p + 5
Label1.Text = "2. How many phases are there in Scrum ?"
Label2.Text = "A. Two" + vbCrLf + "B. Three" + vbCrLf + "C. Four" + vbCrLf + "D. Scrum is an agile method which means it does not have phases"
ElseIf String.Equals(Label1.Text, "2. How many phases are there in Scrum ?") Then 'hi
p = p + 0
Label1.Text = "3. Which of the following does not apply to agility to a software process?"
Label2.Text = "A. Uses incremental product delivery strategy" + vbCrLf + "B. Only essential work products are produced" + vbCrLf + "C. Eliminate the use of project planning and testing" + vbCrLf + "D. All of the mentioned"
ElseIf String.Equals(Label1.Text, "3. Which of the following does not apply to agility to a software process?") Then 'Yeah
p = p + 0
Label1.Text = "4. Agile Software Development is based on?"
Label2.Text = "A. Incremental Development" + vbCrLf + "B. Iterative Development" + vbCrLf + "C. Linear Development" + vbCrLf + "D. Both Incremental and Iterative Development"
ElseIf String.Equals(Label1.Text, "4. Agile Software Development is based on?") Then 'Nope
p = p + 0
Label1.Text = "5. Which on of the following is not an agile method?"
Label2.Text = "A. 4GT" + vbCrLf + "B. XP" + vbCrLf + "C. AUP" + vbCrLf + "D. All of the mentioned"
ElseIf String.Equals(Label1.Text, "5. Which on of the following is not an agile method?") Then 'Who are you?
