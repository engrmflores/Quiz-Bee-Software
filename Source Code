Private Sub Button4_Click(sender As Object, e As EventArgs) Handles Button4.Click
Dim easy1 As Integer
Dim average2 As Integer
Dim hard3 As Integer
easy1 = Integer.Parse(pt.Text)
average2 = Integer.Parse(pt2.Text)
hard3 = Integer.Parse(pt3.Text)
Dim sum As Integer
sum = easy1 + average2 + hard3
ListBox1.Items.Add(sum)
End Sub
Private Sub pt2_Click(sender As Object, e As EventArgs) Handles pt2.Click
End Sub
Private Sub ListBox1_SelectedIndexChanged(sender As Object, e As EventArgs) Handles ListBox1.SelectedIndexChanged
End Sub
End Class
Public Class A
Public Property p As Integer = 0
'ans ABCDA
Private Sub easy_Load(sender As Object, e As EventArgs) Handles MyBase.Load
Label1.Text = "1. Identify the disadvantage of Spiral Model.?"
Label2.Text = "A. Doesnâ€™t work well for smaller projects" + vbCrLf + "B. High amount of risk analysis" + vbCrLf + "C. Strong approval and documentation control" + vbCrLf + "D. Additional Functionality can be added at a later date"
Bt.Visible = "False"
End Sub
Private Sub Ab_Click(sender As Object, e As EventArgs) Handles Ab.Click
If String.Equals(Label1.Text, "1. Identify the disadvantage of Spiral Model.?") Then ' 1
p = p + 1
Label1.Text = "2. Which one of the following is not a software process quality?"
Label2.Text = "A. Productivity" + vbCrLf + "B. Portability" + vbCrLf + "C. Timeliness" + vbCrLf + "D. Visibility"
ElseIf String.Equals(Label1.Text, "2. Which one of the following is not a software process quality?") Then '2
p = p + 0
Label1.Text = "3. _____________&_____________ are two kinds of software products.?"
Label2.Text = "A. CAD, CAM" + vbCrLf + "B. Firmware, Embedded" + vbCrLf + "C. Generic, Customised" + vbCrLf + "D. None of the mentioned"
ElseIf String.Equals(Label1.Text, "3. _____________&_____________ are two kinds of software products.?") Then ' 3
p = p + 0
Label1.Text = "4. Purpose of process is to deliver software?"
Label2.Text = "A. in time" + vbCrLf + "B. with acceptable quality" + vbCrLf + "C. that is cost efficient" + vbCrLf + "D. both in time & with acceptable quality"
ElseIf String.Equals(Label1.Text, "4. Purpose of process is to deliver software?") Then ' 4
p = p + 0
Label1.Text = "5. How is Incremental Model different from Spiral Model?"
Label2.Text = "A. Progress can be measured for Incremental Model" + vbCrLf + "B. Changing requirements can be accommodated in Incremental Model" + vbCrLf + "C. Users can see the system early in Incremental Model" + vbCrLf + "D. All of the mentioned"
ElseIf String.Equals(Label1.Text, "5. How is Incremental Model different from Spiral Model?") Then ' 5
p = p + 1
Label1.Text = "Finished"
Bt.Visible = "True"
Else
p = p + 0
End If
End Sub
