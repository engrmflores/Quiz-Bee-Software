ElseIf String.Equals(Label1.Text, "4. Data Store Symbol in DFD represents a___________?") Then 'Nope
p = p + 0
Label1.Text = "5. Which of the following is a function of CASE Tool?"
Label2.Text = "A. Supporting Structured analysis and design (SA/SD)" + vbCrLf + "B. Maintaining the data dictionary" + vbCrLf + "C. Checking whether DFDs are balanced or not" + vbCrLf + "D. All of the mentioned"
ElseIf String.Equals(Label1.Text, "5. Which of the following is a function of CASE Tool?") Then 'Who are you?
p = p + 3
Label1.Text = "Finished"
Bt.Visible = "True"
Else
p = p + 0
End If
